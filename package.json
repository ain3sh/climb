{
  "name": "junglectl",
  "version": "1.0.0",
  "description": "Interactive terminal UI for MCPJungle - eliminates flag memorization and manual JSON crafting",
  "type": "module",
  "main": "./dist/index.js",
  "bin": {
    "junglectl": "./dist/index.js",
    "jctl": "./dist/index.js"
  },
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "watch": "tsc --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "mcp",
    "mcpjungle",
    "cli",
    "interactive",
    "terminal",
    "tui"
  ],
  "author": "Ainesh",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.8.4",
    "@inquirer/search": "^3.2.0",
    "chalk": "^5.4.1",
    "cli-table3": "^0.6.5",
    "commander": "^12.1.0",
    "node-pty": "^1.1.0-beta22",
    "ora": "^8.2.0",
    "strip-ansi": "^7.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.17.1",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0"
  }
}
